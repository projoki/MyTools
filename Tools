sh kalinethunter

cd ~/

echo -e $red" T E R I N S T A L L "

fi

if [ $act = 7 ] || [ $act = 07 ]

then

clear

echo -e $red" Installing Hydra "

sleep 1

apt update && apt install -y wget

apt install hydra

wget http://scrapmaker.com/download/data/wordlists/dictionaries/rockyou.txt

