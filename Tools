read -p "[Projoki@Tools]> " act;


if [ $act = 1 ] || [ $act = 01 ]

then

clear

echo -e $red" Installing OSIF "

sleep 1

apt update && apt upgrade

apt install php

apt install git

git clonehttps://github.com/projoki/OSIF-Projoki

cd ~/

echo -e $red" T E R I N S T A L L "

fi
if [ $act = 2 ] || [ $act = 02 ]

then

clear

echo -e $red" Installing Hunner "

sleep 1

apt-get update && apt-get upgrade

apt install python

apt install git

git clone https://github.com/Senitopeng/fbbrute

cd ~/

