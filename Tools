clear


#This colour

blue='\e[0;34'

cyan='\e[0;36m'

green='\e[0;34m'

okegreen='\033[92m'

lightgreen='\e[1;32m'

white='\e[1;37m'

red='\e[1;31m'

yellow='\e[1;33m'


###################################################

# CTRL C

###################################################

trap ctrl_c INT

ctrl_c() {

clear

echo -e $red"[#]> (Ctrl + C ) Detected, Trying To Exit ... "

sleep 1

echo ""

echo -e $yellow"[#]> Thank You For Using My Tools ... "

sleep 1

echo ""

echo -e $white"[#]> Im Here ... "

read enter

exit

}


# Isi oc :*

